FUNCTION PGCNOM(PGCNOMCOD,NIVEL)
    LOCAL NUMSEL:=SELECT()
    LOCAL NUMSELPGC:=SELEC("PGC")
    IF VALTYPE(PGCNOMCOD)="C"
        PGCNOMCOD:=VAL(PGCNOMCOD)
    ENDIF
    IF .NOT. FILE(RUTASUIZO+"\PGC.DBF") .OR. ;
        .NOT. FILE(RUTASUIZO+"\PGC.CDX")
        PGCNOMCOD:={" "," "," "}
    ELSE
        IF NUMSELPGC=0
            AbrirDBF("PGC",,,"Exclusive",RUTASUIZO)
        ENDIF
        SELEC PGC
        PGCNOMCODA:=SPACE(4)
        PGCNOMCODB:=SPACE(40)
        PGCNOMCODC:=SPACE(100)
        PGCNOMCOD2:=PADR(LTRIM(STR(PGCNOMCOD)) ,NIVEL,"0")
        FOR N=NIVEL TO 1 STEP -1
            IF N<NIVEL .AND. SUBSTR(PGCNOMCOD2,N+1,1)<>"0"
                EXIT
            ENDIF
            PGCNOMCOD3:=PADR(LEFT(PGCNOMCOD2,N),4," ")
            SEEK PGCNOMCOD3
            IF .NOT. EOF()
                PGCNOMCODA:=CODPGC
                PGCNOMCODB:=NOMPGC1
                PGCNOMCODC:=NOMPGC2
                EXIT
            ENDIF
        NEXT
        PGCNOMCOD:={PGCNOMCODA,PGCNOMCODB,PGCNOMCODC}
        IF NUMSELPGC=0
            SELEC PGC
            USE
        ENDIF
    ENDIF
    SELEC &NUMSEL
RETURN(PGCNOMCOD)
